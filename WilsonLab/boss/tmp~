                                                <div id="fb-root"></div><script type="text/javascript">
var toggle = function() {
    toggleDIV('addAlbumComment','toggleAlbumComment', 'block');
    redoCaptcha('Album','');};

YE.onAvailable('toggleAlbumComment', function() {
    function showCommentEntry(p_oEvent) {
        toggle();
                SM.FBAttach(function(){

                var settings = {settings: {useWelcomeMessage : false, requiredPermissions : 'email'}};

                var events = {events : {eventsComplete : fbLogin, logout : fbLogout}};

                var cssOnWrapUp = {onWrapUp :{'fbButton' : 'display'}};

                fbObject.config(settings);
                fbObject.config(events);
                fbObject.config(cssOnWrapUp);


                fbObject.init();
                //The ID of where you want the button
                fbObject.renderButton('facebookButton');

            }); //End SM.FBAttach
        }

    SM.buttons.addCommentButton = new YAHOO.widget.Button({
        id: 'addcommentButton',
        label: 'Add Comment', 
        container: 'toggleAlbumComment',
        className: 'sm-button sm-button-small addcommentButton glyphButton',
        onclick: { fn: showCommentEntry }
    });

});   
        
var fbLogin = function(response){
    var fbUserData = fbObject.getUserData(
        function(userProfile, safeUserData, response){

            UserData  = '<div id="user" class="user" style="float:left;padding:5px;">';
            UserData += fbObject.renderWelcomeMessage(undefined,undefined,true);
            UserData += '<input type="hidden" name="ServiceID" id="ServiceID"  value="1" />';
            UserData += '<input type="hidden" name="albumCommentEmail" id="albumCommentEmail" value="' + userProfile.accountEmail + '" />';
            UserData += '<input type="hidden" name="SocialID"  id="SocialID" value="' + userProfile.accountID + '" />';
            UserData += '<input type="hidden" name="albumCommentName" id="albumCommentName" value="' + userProfile.accountName + '" />';
            UserData += '<input type="hidden" name="albumCommentLink" id="albumCommentLink" value="' + userProfile.accountLink + '" />';
            UserData += '</div><br />';

            YD.get('user').innerHTML = UserData;

            YD.get('galleryCommentCaptcha').innerHTML = '<input type="hidden" name="noCaptchaNeeded" id="noCaptchaNeeded" value="true" />';

        } //end function
    );
} //end fbLogin

function fbLogout(){
        YD.addClass(document.body, 'masked');
        //YD.get('FBConnectButton').innerHTML = '<span id="facebookButton"></span>';
        YD.get('user').innerHTML = '<span class="title">Name:</span> <input type="text" name="albumCommentName" id="albumCommentName" style="width: 100px" /> <span class="title">Email:</span> <input type="text" name="albumCommentEmail" id="albumCommentEmail" style="width: 100px" /> <span class="title">Link:</span>  <input type="text" name="albumCommentLink" id="albumCommentLink" style="width: 100px" />';
        fbObject.renderButton('facebookButton',undefined,undefined,undefined,true);
        YD.removeClass(document.body, 'masked');
}


function disableField(){
    if(YD.get('albumCommentEmail').disabled==true) {
        YD.get('albumCommentEmail').disabled=false;
        YD.get('albumCommentEmail').value='';
    } else {
        YD.get('albumCommentEmail').disabled=true;
        YD.get('albumCommentEmail').value='';
    }
}

</script>
