          (function() {
            var parts = window.location.href.split("#");
            if (parts.length < 2 || YD.hasClass(document.body, 'smugmug_ajax')
            	|| YD.hasClass(document.body, 'singleImage')
            	|| YD.hasClass(document.body, 'allthumbs_stretch')
            	|| YD.hasClass(document.body, 'journal_new')) {
            	return;
            }
            if (YD.hasClass(document.body,'galleryPage')
                || YD.hasClass(document.body,'keywordPage')
                || YD.hasClass(document.body,'datePage')
                || YD.hasClass(document.body,'popularGallery')
                || YD.hasClass(document.body,'popularImage')
                || YD.hasClass(document.body,'communityKeyword')) {
                var goHere = SM.util.normalizeURL();
                if (/(\w+\-[a-zA-Z0-9]+\-LB$)|(&lb=1&s=[a-zA-Z0-9]+)/.test(parts[1])) {
                    if (parts[1].charAt(0) == '!') {
                        // If it's a hash bang url, encode it so the ampersands don't break it
                        parts[1] = '!' + escape(parts[1].substr(1));
                    }
                    goHere += ('?c=1#' + parts[1]);
                }
                location.href = goHere;
            }
          })();
    </script>
