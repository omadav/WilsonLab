        // IE has issues with setting cookies when sites are framed, so logging in and using the cart do not work. So we have to break frames!
                    // -->
            (function() {
                    if(typeof readCookie  != "undefined") {
                        var sRes;

                        if (typeof(screen) == 'object'){
                            sRes = screen.width + 'x' + screen.height;
                        }

                        if (readCookie('res') != sRes) {
                            createCookie('res', sRes, 365);
                        }
                    }
                })();
        </script>
